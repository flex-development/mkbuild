/**
 * @file Type Aliases - GeneratedCodeOption
 * @module mkbuild/types/GeneratedCodeOption
 */

import type { GeneratedCodeOptions, GeneratedCodePreset } from 'rollup'

/**
 * Union of values used to determine language features mkbuild (rollup) can
 * safely use in generated code.
 *
 * @see {@linkcode GeneratedCodeOptions}
 * @see {@linkcode GeneratedCodePreset}
 */
type GeneratedCodeOption = GeneratedCodeOptions | GeneratedCodePreset

export type { GeneratedCodeOption as default }
