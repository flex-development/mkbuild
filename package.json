{
  "name": "@flex-development/mkbuild",
  "description": "An esbuild based file-to-file transformer and bundler",
  "version": "1.0.0-alpha.9",
  "keywords": [
    "bundle",
    "bundler",
    "dts",
    "esbuild",
    "file-to-file",
    "jsx",
    "react",
    "transpile",
    "transpiler",
    "typescript"
  ],
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/flex-development/mkbuild",
  "repository": "https://github.com/flex-development/mkbuild.git",
  "bugs": "https://github.com/flex-development/mkbuild/issues",
  "author": {
    "name": "Lexus Drumgold",
    "url": "https://github.com/unicornware"
  },
  "publishConfig": {
    "access": "public",
    "directory": "./"
  },
  "type": "module",
  "files": [
    "dist",
    "patches"
  ],
  "bin": "./dist/cli.mjs",
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json",
    "./plugins/*": "./dist/plugins/*.mjs"
  },
  "imports": {
    "#src": "./src/index.ts",
    "#src/*": "./src/*.ts"
  },
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "typesVersions": {
    "*": {
      "./plugins/*": [
        "./dist/plugins/*.d.mts"
      ]
    }
  },
  "scripts": {
    "build": "node --loader=./loader.mjs ./src/cli",
    "check:ci": "yarn dedupe --check && yarn check:format && yarn check:lint && yarn check:spelling && yarn typecheck && yarn test:cov && NODE_ENV=production yarn pack -o %s-%v.tgz && yarn clean:pack",
    "check:format": "prettier --check .",
    "check:lint": "eslint --exit-on-fatal-error --ext cjs,cts,gql,js,json,json5,jsonc,jsx,md,mjs,mts,ts,tsx,yml --max-warnings 0 .",
    "check:spelling": "cspell lint --color --no-progress --relative $@ \"**\"",
    "check:types": "tsc -p tsconfig.typecheck.json",
    "check:upgrades": "yarn upgrade-interactive",
    "clean:build": "trash ./{dist,*.tgz}",
    "clean:coverage": "trash ./coverage",
    "clean:modules": "trash ./.yarn/{cache,*.gz} ./node_modules",
    "clean:pack": "trash ./*.tgz",
    "clean:test": "yarn clean:coverage && trash ./__tests__/report.json",
    "conventional-changelog": "node --loader=./loader.mjs ./node_modules/.bin/conventional-changelog -n ./changelog.config.cts",
    "fix:cg": "yarn fix:format && yarn fix:lint",
    "fix:dedupe": "yarn dedupe --strategy=highest",
    "fix:format": "prettier --cache --write .",
    "fix:lint": "yarn check:lint --cache --fix",
    "postinstall": "chmod +x .husky/* && husky install",
    "postpack": "toggle-scripts +postinstall",
    "postpublish": "toggle-scripts +prepack",
    "prepack": "toggle-scripts -postinstall && yarn build",
    "prepublishOnly": "toggle-scripts -prepack",
    "recommended-bump": "conventional-recommended-bump --preset=conventionalcommits --tag-prefix=$(jq .tagPrefix package.json -r) --verbose",
    "release": "bash ./scripts/release.sh",
    "test": "vitest run",
    "test:cov": "yarn clean:coverage; yarn test --coverage",
    "test:cov:serve": "serve ./coverage/lcov-report",
    "test:watch": "vitest",
    "typecheck": "vitest typecheck --run",
    "typecheck:watch": "vitest typecheck"
  },
  "dependencies": {
    "@flex-development/mlly": "1.0.0-alpha.2",
    "@flex-development/pkg-types": "1.0.0",
    "@flex-development/tutils": "6.0.0-alpha.7",
    "colorette": "2.0.19",
    "consola": "2.15.3",
    "cosmiconfig": "7.0.1",
    "defu": "6.1.0",
    "escape-string-regexp": "5.0.0",
    "fs-extra": "10.1.0",
    "globby": "13.1.2",
    "pathe": "0.3.8",
    "pretty-bytes": "6.0.0",
    "tsconfig-paths": "4.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.3.0",
    "@commitlint/config-conventional": "17.3.0",
    "@faker-js/faker": "7.6.0",
    "@graphql-eslint/eslint-plugin": "3.13.1",
    "@types/chai": "4.3.4",
    "@types/chai-fs": "2.0.2",
    "@types/chai-string": "1.4.2",
    "@types/conventional-changelog": "3.1.1",
    "@types/conventional-changelog-config-spec": "2.1.2",
    "@types/conventional-changelog-writer": "4.0.1",
    "@types/conventional-commits-parser": "3.0.2",
    "@types/dateformat": "5.0.0",
    "@types/eslint": "8.4.10",
    "@types/fs-extra": "9.0.13",
    "@types/git-raw-commits": "2.0.1",
    "@types/is-ci": "3.0.0",
    "@types/node-notifier": "8.0.2",
    "@types/prettier": "2.7.1",
    "@types/react": "18.0.26",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "@vates/toggle-scripts": "1.0.0",
    "@vitest/coverage-c8": "0.25.3",
    "@vitest/ui": "0.25.3",
    "chai": "4.3.7",
    "chai-each": "0.0.1",
    "chai-fs": "2.0.0",
    "chai-quantifiers": "1.0.17",
    "chai-string": "1.5.0",
    "conventional-changelog-cli": "2.2.2",
    "conventional-recommended-bump": "6.1.0",
    "cspell": "6.16.0",
    "esbuild": "0.15.17",
    "eslint": "8.29.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-chai-expect": "3.0.0",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-jsdoc": "39.6.4",
    "eslint-plugin-jsonc": "2.5.0",
    "eslint-plugin-markdown": "3.0.0",
    "eslint-plugin-markdownlint": "0.4.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-unicorn": "45.0.1",
    "eslint-plugin-yml": "1.2.0",
    "graphql": "16.6.0",
    "graphql-config": "4.3.6",
    "growl": "1.10.5",
    "husky": "8.0.2",
    "is-ci": "3.0.1",
    "jsonc-eslint-parser": "2.1.0",
    "lint-staged": "13.0.4",
    "memfs": "3.4.12",
    "node-notifier": "10.0.1",
    "prettier": "2.8.0",
    "prettier-plugin-sh": "0.12.8",
    "pretty-format": "29.3.1",
    "radash": "9.5.0",
    "react": "18.2.0",
    "sade": "1.8.1",
    "serve": "14.1.2",
    "styled-components": "5.3.6",
    "trash-cli": "5.0.0",
    "ts-dedent": "2.2.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.3",
    "version-bump-prompt": "6.1.0",
    "vite": "3.2.4",
    "vite-tsconfig-paths": "3.6.0",
    "vitest": "0.25.3",
    "yaml-eslint-parser": "1.1.0"
  },
  "peerDependencies": {
    "esbuild": ">=0.15.0",
    "typescript": ">=3.7.2"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "resolutions": {
    "@ardatan/sync-fetch": "larsgw/sync-fetch#head=worker_threads",
    "esbuild": "0.15.17",
    "globby@npm:13.1.2": "patch:globby@npm%3A13.1.2#patches/globby+13.1.2.patch",
    "pathe": "0.3.8",
    "radash@npm:9.5.0": "patch:radash@npm%3A9.5.0#patches/radash+9.5.0.dev.patch",
    "vite": "3.2.4",
    "vitest@npm:0.25.3": "patch:vitest@npm%3A0.25.3#patches/vitest+0.25.3.dev.patch"
  },
  "engines": {
    "node": ">=14",
    "yarn": "4.0.0-rc.14"
  },
  "packageManager": "yarn@4.0.0-rc.14",
  "readme": "README.md",
  "sideEffects": false,
  "tagPrefix": ""
}
