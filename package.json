{
  "name": "@flex-development/mkbuild",
  "description": "An esbuild based file-to-file transformer and bundler",
  "version": "1.0.0-alpha.1",
  "keywords": [
    "bundle",
    "bundler",
    "dts",
    "esbuild",
    "file-to-file",
    "jsx",
    "react",
    "transpile",
    "transpiler",
    "typescript"
  ],
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/flex-development/mkbuild",
  "repository": "https://github.com/flex-development/mkbuild.git",
  "bugs": "https://github.com/flex-development/mkbuild/issues",
  "author": {
    "name": "Lexus Drumgold",
    "url": "https://github.com/unicornware"
  },
  "publishConfig": {
    "access": "public",
    "directory": "./"
  },
  "type": "module",
  "files": [
    "dist",
    "patches",
    "src"
  ],
  "bin": "./dist/cli.mjs",
  "exports": {
    ".": "./dist/index.mjs",
    "./config/*": "./dist/config/*.mjs",
    "./interfaces": "./dist/interfaces/index.mjs",
    "./interfaces/*": "./dist/interfaces/*.mjs",
    "./make": "./dist/make.mjs",
    "./package.json": "./package.json",
    "./plugins/*": "./dist/plugins/*.mjs",
    "./types": "./dist/types/index.mjs",
    "./types/*": "./dist/types/*.mjs",
    "./utils/*": "./dist/utils/*.mjs"
  },
  "imports": {
    "#src": "./src/index.ts",
    "#src/*": "./src/*.ts"
  },
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*.d.mts",
        "./dist/*/index.d.mts",
        "./dist/index.d.mts"
      ]
    }
  },
  "scripts": {
    "build": "node --loader=./loader.mjs ./src/cli",
    "check:ci": "yarn dedupe --check && yarn check:format && yarn check:lint && yarn check:spelling && yarn check:types && yarn check:types:build && yarn test:cov && NODE_ENV=production yarn pack -o %s-%v.tgz && yarn clean:pack",
    "check:format": "prettier --check .",
    "check:lint": "eslint --exit-on-fatal-error --ext cjs,cts,gql,js,json,json5,jsonc,jsx,md,mjs,mts,ts,tsx,yml --max-warnings 0 .",
    "check:spelling": "cspell lint --color --no-progress --relative $@ \"**\"",
    "check:types": "tsc -p tsconfig.json",
    "check:types:build": "tsc -p tsconfig.build.json",
    "check:upgrades": "yarn upgrade-interactive",
    "clean:build": "trash ./{dist,*.tgz}",
    "clean:modules": "trash ./.yarn/{cache,*.gz} ./node_modules",
    "clean:pack": "trash ./*.tgz",
    "clean:test": "trash ./coverage",
    "conventional-changelog": "node --loader=./loader.mjs ./node_modules/.bin/conventional-changelog -n ./changelog.config.cts",
    "fix:cg": "yarn fix:format && yarn fix:lint",
    "fix:dedupe": "yarn dedupe --strategy=highest",
    "fix:format": "prettier --cache --write .",
    "fix:lint": "yarn check:lint --cache --fix",
    "postinstall": "chmod +x .husky/* && husky install",
    "postpack": "toggle-scripts +postinstall",
    "postpublish": "toggle-scripts +prepack",
    "prepack": "toggle-scripts -postinstall && yarn build",
    "prepublishOnly": "toggle-scripts -prepack",
    "recommended-bump": "conventional-recommended-bump --preset=conventionalcommits --tag-prefix=$(jq .tagPrefix package.json -r) --verbose",
    "release": "bash ./scripts/release.sh",
    "test": "vitest run",
    "test:cov": "yarn test --coverage",
    "test:watch": "vitest"
  },
  "dependencies": {
    "colorette": "2.0.19",
    "consola": "2.15.3",
    "cosmiconfig": "7.0.1",
    "defu": "6.1.0",
    "escape-string-regexp": "5.0.0",
    "fs-extra": "10.1.0",
    "globby": "13.1.2",
    "mlly": "0.5.14",
    "pathe": "0.3.5",
    "pkg-types": "0.3.5",
    "postinstall-postinstall": "2.1.0",
    "pretty-bytes": "6.0.0",
    "tsconfig-paths": "4.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.1.1",
    "@commitlint/config-conventional": "17.1.0",
    "@commitlint/types": "17.0.0",
    "@faker-js/faker": "7.4.0",
    "@flex-development/tutils": "5.0.0",
    "@graphql-eslint/eslint-plugin": "3.10.7",
    "@types/chai": "4.3.3",
    "@types/chai-fs": "2.0.2",
    "@types/chai-string": "1.4.2",
    "@types/conventional-changelog": "3.1.1",
    "@types/conventional-changelog-config-spec": "2.1.1",
    "@types/conventional-changelog-writer": "4.0.1",
    "@types/conventional-commits-parser": "3.0.2",
    "@types/dateformat": "5.0.0",
    "@types/eslint": "8.4.6",
    "@types/fs-extra": "9.0.13",
    "@types/git-raw-commits": "2.0.1",
    "@types/is-ci": "3.0.0",
    "@types/node-notifier": "8.0.2",
    "@types/prettier": "2.7.0",
    "@types/react": "18.0.20",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "5.35.1",
    "@typescript-eslint/parser": "5.35.1",
    "@vates/toggle-scripts": "1.0.0",
    "@vitest/coverage-c8": "0.23.1",
    "@vitest/ui": "0.23.1",
    "chai": "4.3.6",
    "chai-each": "0.0.1",
    "chai-fs": "2.0.0",
    "chai-quantifiers": "1.0.17",
    "chai-string": "1.5.0",
    "conventional-changelog-cli": "2.2.2",
    "conventional-recommended-bump": "6.1.0",
    "cspell": "6.8.0",
    "dotenv": "16.0.1",
    "dotenv-expand": "8.0.3",
    "esbuild": "0.15.9",
    "eslint": "8.23.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-chai-expect": "3.0.0",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-jsdoc": "39.3.6",
    "eslint-plugin-jsonc": "2.4.0",
    "eslint-plugin-markdown": "3.0.0",
    "eslint-plugin-markdownlint": "0.4.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.0.1",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-unicorn": "43.0.2",
    "eslint-plugin-yml": "1.1.0",
    "graphql": "16.6.0",
    "graphql-config": "4.3.5",
    "growl": "1.10.5",
    "husky": "8.0.1",
    "is-ci": "3.0.1",
    "jsonc-eslint-parser": "2.1.0",
    "lint-staged": "13.0.3",
    "memfs": "3.4.7",
    "node-notifier": "10.0.1",
    "prettier": "2.7.1",
    "prettier-plugin-sh": "0.12.8",
    "pretty-format": "29.0.1",
    "radash": "7.1.0",
    "react": "18.2.0",
    "sade": "1.8.1",
    "styled-components": "5.3.5",
    "trash-cli": "5.0.0",
    "ts-dedent": "2.2.0",
    "ts-node": "10.9.1",
    "typescript": "4.8.2",
    "version-bump-prompt": "6.1.0",
    "vite": "3.0.9",
    "vite-tsconfig-paths": "3.5.0",
    "vitest": "0.23.1",
    "vitest-github-actions-reporter": "0.8.1",
    "yaml-eslint-parser": "1.1.0"
  },
  "peerDependencies": {
    "esbuild": ">=0.15.0",
    "typescript": ">=3.7.2"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "resolutions": {
    "@ardatan/sync-fetch": "larsgw/sync-fetch#head=worker_threads",
    "esbuild": "0.15.9",
    "globby@npm:13.1.2": "patch:globby@npm%3A13.1.2#patches/globby+13.1.2.patch",
    "graphql-config": "patch:graphql-config@npm%3A4.3.5#patches/graphql-config+4.3.5.dev.patch",
    "mlly": "patch:mlly@npm%3A0.5.14#patches/mlly+0.5.14.patch"
  },
  "engines": {
    "node": ">=14",
    "yarn": "4.0.0-rc.14"
  },
  "packageManager": "yarn@4.0.0-rc.14",
  "readme": "README.md",
  "tagPrefix": ""
}
